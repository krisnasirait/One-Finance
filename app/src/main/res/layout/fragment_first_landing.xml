<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.branding.fragmentlanding.FirstLandingFragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/tvYourData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Your Data Security is Our Priority"
        android:gravity="center_horizontal"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:layout_marginTop="77dp"
        android:fontFamily="@font/poppinssemibold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvWeWell"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="We will show you the best security at every experience on this app"
        android:gravity="center_horizontal"
        android:layout_marginHorizontal="16dp"
        android:textColor="@color/white"
        android:textSize="14sp"
        android:layout_marginTop="27dp"
        android:fontFamily="@font/poppinsregular"
        app:layout_constraintTop_toBottomOf="@id/tvYourData" />

</androidx.constraintlayout.widget.ConstraintLayout>